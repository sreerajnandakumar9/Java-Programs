package collections;

import java.util.stream.IntStream;

public class PrintRange {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		//IntStream.rangeClosed(0, 50).filter(n->n%2==0).forEach(System.out::println);
		IntStream.rangeClosed(0, 30).filter(n->n%2!=0).forEach(System.out::println);

	}

}
